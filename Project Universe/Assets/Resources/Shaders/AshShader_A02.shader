				float4 temp_cast_0 = (( break296.a - _SmoothnessLDodge )).xxxx;
				float grayscale348 = Luminance(CalculateContrast(_SmoothnessContrast,temp_cast_0).rgb);
				
				surfaceDescription.Normal = tex2DNode315;
